{
    "manifest_version": 3,
    "name": "本番まもる君",
    "version": "0.2.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": [
        "*://*/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icon_128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
